{
    "name": "smartweb/nats",
    "description": "PHP implementation of the NATS messaging protocol using the CloudEvents specification.",
    "authors": [
        {
            "name": "Nicolai AgersbÃ¦k",
            "email": "na@smartweb.dk"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.smartweb.io/"
        }
    ],
    "type": "project",
    "require": {
        "php": ">=7.1",
        "smartweb/cloud-events-nats": "^1.1",
        "symfony/serializer": "^4.1",
        "symfony/options-resolver": "^4.1"
    },
    "require-dev": {
        "ext-curl": "*",
        "ext-mbstring": "*",
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "SmartWeb\\Nats\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmartWeb\\Nats\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.1.20",
            "ext-curl": "0.1",
            "ext-mbstring": "0.1"
        }
    }
}
