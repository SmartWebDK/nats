{
    "name": "smartweb/nats",
    "description": "PHP implementation of the NATS messaging protocol using the CloudEvents specification.",
    "authors": [
        {
            "name": "Nicolai AgersbÃ¦k",
            "email": "na@smartweb.dk"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.smartweb.io/"
        }
    ],
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "ext-bcmath": "*",
        "google/protobuf": "^3.6",
        "smartweb/php-events-base": "^1.1.1",
        "smartweb/php-nats-streaming": "^0.3",
        "smartweb/phpnats": "^0.9"
    },
    "require-dev": {
        "ext-curl": "*",
        "ext-mbstring": "*",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^7",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "SmartWeb\\Nats\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmartWeb\\Nats\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.1.20",
            "ext-bcmath": "0.1",
            "ext-curl": "0.1",
            "ext-mbstring": "0.1"
        }
    }
}
