{
    "name": "smartweb/nats",
    "description": "PHP implementation of the NATS messaging protocol using the CloudEvents specification.",
    "authors": [
        {
            "name": "Nicolai AgersbÃ¦k",
            "email": "na@smartweb.dk"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nicolai-agersbaek/cloud-events"
        }
    ],
    "type": "project",
    "require": {
        "php": ">=7.1.20",
        "smartweb/cloud-events": "dev-master",
        "repejota/nats": "^0.8.7",
        "byrnedo/php-nats-streaming": "^0.2.4",
        "symfony/serializer": "^4.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "SmartWeb\\Nats\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmartWeb\\Nats\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "7.1.20",
            "ext-mbstring": "0.1",
            "ext-dom": "0.1"
        }
    }
}
